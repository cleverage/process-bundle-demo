{
    "name": "cleverage/process-bundle-demo",
    "type": "project",
    "description": "A demo project using cleverage/process-bundle & cleverage/*-process-bundle bridges",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "cleverage/doctrine-process-bundle": "dev-prepare-release",
        "cleverage/process-bundle": "dev-prepare-release",
        "cleverage/ui-process-bundle": "dev-prepare-release",
        "symfony/flex": "^v2.3"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.51",
        "symfony/stopwatch": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "friendsofphp/php-cs-fixer": "^3.34",
        "phpstan/phpstan": "^1.10",
        "roave/security-advisories": "dev-latest",
        "symfony/debug-bundle": "^6.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "preferred-install": {
            "cleverage/*": "source",
            "*": "dist"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cleverage/ui-process-bundle.git"
        }
    ],
    "minimum-stability": "dev"
}
